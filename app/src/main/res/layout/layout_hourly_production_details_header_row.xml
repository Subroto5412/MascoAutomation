        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/daily_attendance_item_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginBottom="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/activity_hourly_production_details_list_header_cl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_31sdp"
                android:layout_marginBottom="1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginEnd="@dimen/_8sdp">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_sl_tv"
                    android:layout_width="@dimen/_32sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="SL"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_hour_tv"
                    android:layout_width="@dimen/_47sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Hour"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_sl_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_cutting_tv"
                    android:layout_width="@dimen/_47sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Cutting"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_hour_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_line_input_tv"
                    android:layout_width="@dimen/_48sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Line Input"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_cutting_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_sewing_outout_tv"
                    android:layout_width="@dimen/_48sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Sewing Output"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_line_input_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_iron_tv"
                    android:layout_width="@dimen/_48sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Iron"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_11sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_sewing_outout_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_folding_tv"
                    android:layout_width="@dimen/_48sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Folding"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_11sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_iron_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_poly_tv"
                    android:layout_width="@dimen/_48sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Poly"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_11sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_folding_tv"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/hourly_production_details_carton_tv"
                    android:layout_width="@dimen/_47sdp"
                    android:layout_height="@dimen/_31sdp"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/hourly_production_details_bg_round"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center"
                    android:text="Carton"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_11sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hourly_production_details_poly_tv"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!--</ScrollView>-->


        <!-- </androidx.cardview.widget.CardView>-->
    </androidx.constraintlayout.widget.ConstraintLayout>
